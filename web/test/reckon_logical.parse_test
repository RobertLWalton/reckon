// simple expression assignment
//
X = 5
{"": .terminator = "<LF>", .initiator = [$ LOGICAL_LINE $]|{|X|} =
     {|5|}|}
X < 10
{"": .terminator = "<LF>", .initiator = [$ LOGICAL_LINE $]|{|X|} <
     {|10|}|}
X < 2
{"": .terminator = "<LF>", .initiator = [$ LOGICAL_LINE $]|{|X|} <
     {|2|}|}
2 < X < 10
{"": .terminator = "<LF>", .initiator = [$ LOGICAL_LINE $]|{|2|} < {|X|}
     < {|10|}|}
next X = 13
{"": .terminator = "<LF>", .initiator = [$ LOGICAL_LINE $]|{|next X|} =
     {|13|}|}
2 < X < 10
{"": .terminator = "<LF>", .initiator = [$ LOGICAL_LINE $]|{|2|} < {|X|}
     < {|10|}|}
