*PARSER*:
    trace [+ parser output]
    trace [+ subexpression lines]
    trace [+ subexpression elements]
    trace [+ bracketed subexpressions]

!@1 :=:
    kind = woman
    name = Jill
    husband = @2 = wife
!@2 :=:
    kind = man
    name = Jack
    wife = @1 = husband

// Do not use an operator in the following or the trace
// will collapse the @1 and @2.
//
INPUT ID 1 : OUTPUT ID 2 IS @1
INPUT ID 2 : OUTPUT ID 1 IS @2

!@11 :=:
    kind = woman
    name[-] = Jill
    husband[*] = @12 = wife
!@12 :=:
    kind = man
    name[+] = Jack
    wife[@] = @11 = husband

INPUT ID 11 : OUTPUT ID 4 IS @11
INPUT ID 12 : OUTPUT ID 3 IS @12

!@13 :=:
    kind = boy
    name = giddy
    parent = {* @11, @12 *} = child

INPUT ID 11 : OUTPUT ID 4 IS @11
INPUT ID 12 : OUTPUT ID 3 IS @12
INPUT ID 13 : OUTPUT ID 5 IS @13

!@21 :=:
    x = [< "{" "[" >]
    x y = 5
    [< x y >] = 6
INPUT ID 21 IS @21

!@22 :=:
    x = y 1
    x = y 2
    z = {* y 1, y 2 *}
INPUT ID 22 IS @22

!@23 :=:
    x
    no y
INPUT ID 23 IS @23

